@page ﻿"/actualizarConta"

<h3>Actualizar Fecha</h3>

<div>
    <label for="month">Mes:</label>
    <input type="number" id="month" @bind="Month" min="1" max="12" />

    <label for="year">Año:</label>
    <input type="number" id="year" @bind="Year" min="2000" max="2100" />

    <button @onclick="ActualizarSaldos">Actualizar</button>
</div>



